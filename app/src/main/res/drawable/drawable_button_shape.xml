<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">
    <size
        android:width="200dp"
        android:height="150dp" />
    <solid android:color="#f20" />
    <padding
        android:bottom="5dp"
        android:left="5dp"
        android:right="5dp"
        android:top="5dp" />
    <corners
        android:bottomRightRadius="20dp"
        android:topLeftRadius="20dp" />
    <stroke
        android:width="2dp"
        android:color="#000000"
        android:dashGap="20dp"
        android:dashWidth="50dp"/>
    <gradient
        android:angle="135"
        android:centerColor="#f2ebe9"
        android:startColor="#313caf"
        android:endColor="#ac9c1e"/>
</shape>